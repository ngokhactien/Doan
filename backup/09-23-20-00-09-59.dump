--
-- Name: current_assets _RETURN; Type: RULE; Schema: public; Owner: postgres
--

CREATE OR REPLACE VIEW public.current_assets AS
 SELECT asset.name,
    asset.symbol,
    asset."order",
    asset.symbol_icon,
    asset.id
   FROM (public.asset
     JOIN public.signal ON ((asset.id = signal.asset_id)))
  WHERE ((signal.status)::text = 'active'::text)
  GROUP BY asset.id;


